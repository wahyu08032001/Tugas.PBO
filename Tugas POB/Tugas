public class Tugas1 {
    public static void main(string[] args){
        for (int x=0; x<5; x++){
            for (int y=0; y<5; y++) {
                if (y==0 || y == 2 || == 4){
                    system.out.print("x ");
                }else {
                    system.out.print("0 ");

                    }
                }
                system.out.println();
            }
        }
    }
}
